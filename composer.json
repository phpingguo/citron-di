{
    "name": "phpeel/citron-di",
    "description": "Dependency injection container library wrapper.",
    "license": "MIT",
    "keywords": [
        "di",
        "di container",
        "dependency injection",
        "dependency injection container"
    ],
    "homepage": "https://github.com/phpeel/citron-di",
    "authors": [
        {
            "name" : "Citron DI Contributors",
            "homepage": "https://github.com/phpeel/citron-di/contributors"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "aura/di": "~1.0",
        "symfony/yaml": "2.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "squizlabs/php_codesniffer": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Phpeel\\CitronDI\\": "src/",
            "Phpeel\\CitronDI\\Tests\\": "tests/src/"
        }
    }
